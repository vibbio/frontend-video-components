{
  "name": "@vibbio/slideshow-player",
  "version": "0.1.5",
  "description": "A react component for playing a variety of URLs, including file paths, YouTube, Facebook, SoundCloud, Streamable, Vidme, Vimeo and Wistia",
  "main": "lib/component/VideoComponent.js",
  "files": [
    "/lib"
  ],
  "typings": "index.d.ts",
  "scripts": {
    "clean": "rimraf lib demo",
    "start": "webpack-dev-server",
    "lint": "standard --verbose | snazzy",
    "test": "cross-env NODE_ENV=test babel-node ./node_modules/karma/bin/karma start test/karma.config.js",
    "coverage": "codecov",
    "build:scss": "mkdir lib/component/styling && cp src/component/styling/*.scss lib/component/styling/",
    "build:lib": "cross-env NODE_ENV=production babel src -d lib --ignore src/demo",
    "build:demo": "cross-env NODE_ENV=production webpack --config webpack.demo.babel.js",
    "build:dist": "cross-env NODE_ENV=production webpack --config webpack.dist.babel.js",
    "update-changelog": "auto-changelog --package --template compact",
    "prepublishOnly": "npm run build:lib && npm run build:scss && npm run build:dist",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vibbio/react-player/tree/time-marker"
  },
  "author": "Pete Cook <pete@cookpete.com> (http://github.com/cookpete)",
  "license": "CC0-1.0",
  "homepage": "https://github.com/CookPete/react-player",
  "devDependencies": {
    "auto-changelog": "^1.7.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^4.0.2",
    "codecov": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "mocha": "^4.1.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "3.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.7",
    "screenfull": "^3.3.3",
    "snazzy": "^7.1.1",
    "standard": "^10.0.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deepmerge": "^2.1.1",
    "load-script": "^1.0.0",
    "prop-types": "^15.6.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-es3-member-expression-literals",
          "transform-es3-property-literals"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/*"
    ]
  }
}
